language: bash
git:
  depth: 1
  submodules: true
matrix:
  include:
    - os: windows
before_script:
  - cd msvc6
  - git clone --depth=1 https://github.com/FWGS/shtudio
  - git clone --depth=1 https://github.com/a1batross/Xash3D_original
  - 7z x shtudio/shtudio.7z
script:
  - cmd "/c compile.bat"
after_script:
  - sh continious_upload.sh menudll_orig menu.dll menu.pdb
